{
    "name": "decodelabs/coercion",
    "description": "Simple tools for managing PHP types",
    "type": "library",
    "keywords": [ "type", "data", "structure" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/exceptional": "^0.5.2"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7",
        "nesbot/carbon": "^2|^3"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
